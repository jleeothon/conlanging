{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"lint": {},
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"build/**"
			]
		},
		"test": {
			"dependsOn": [
				"build"
			],
			"inputs": [
				"src/**",
				"test/**"
			]
		},
		"start": {
			"dependsOn": [
				"build"
			]
		}
	}
}
